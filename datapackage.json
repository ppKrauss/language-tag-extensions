{
  "name": "language-tag-extensions",
  "title": "Unicode Common Locale Data",
  "sources": [
    {
      "name":"Unicode Consortium",
      "web": "http://www.unicode.org/Public/cldr/latest/core.zip"
    }
  ],
  "licenses": [
	  {
		  "id": "odc-pddl",
		  "name": "Public Domain Dedication and License",
		  "version": "1.0",
		  "url": "http://opendatacommons.org/licenses/pddl/1.0/"
	  }
	],
  "resources": [
    {
      "name": "language-tag-extensions",
      "path": "data/language-tag-extensions.csv",
      "mediatype": "text/csv",
      "schema": {
        "fields": [
          {
            "name": "lang",
            "description":"IANA/Unicode language-tag-extension",
            "type": "string"
          },
          {
            "name": "langType",
            "description":"ISO 2 letter alpha-2 language code",
            "type": "string"
          },
          {
            "name": "territory",
            "description":"ISO3166-1-Alpha-2 country code",
            "type": "string"
          },
          {
            "name": "revGenDate",
            "description":"revision date (format ISO data)",
            "type": "string"
          },
          {
            "name": "defs",
            "description":"number of definitions",
            "type": "integer"
          },
          {
            "name": "file",
            "description":"file-name of the locale descriptor",
            "type": "string"
          }
	     ]
      }
    }
  ]
}